{
  "name": "eventium",
  "version": "0.0.0",
  "description": "Event scheduling app",
  "main": "src/server.js",
  "scripts": {
    "start": "npm run build && babel-node src/server/server.js",
    "start:dev": "npm run stop-service && export NODE_ENV=development && npm run build:dev && nodemon --exec babel-node -- src/server/server.js",
    "build": "webpack -p",
    "build:dev": "webpack -d",
    "build:dev:watch": "webpack -d --watch",
    "populate": "node src/server/bootstrap/populateDb.js",
    "stop-service": "sh ./src/cli/stop-eventium-service.bash"
  },
  "repository": {
    "type": "git",
    "url": "git@csil-git1.cs.surrey.sfu.ca:callback-hell/term-project.git"
  },
  "keywords": [
    "event",
    "app"
  ],
  "author": "Erik Nummela, Tian Lin Tan, Oleg Matvejev",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "body-parser": "^1.17.1",
    "connect-session-sequelize": "^4.1.0",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "form-data": "^2.1.2",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.18.1",
    "multer": "^1.3.0",
    "node-uuid": "^1.4.8",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.1.5",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^2.6.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sequelize": "^3.30.3",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3"
  },
  "devDependencies": {
    "babel-loader": "^6.4.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "nodemon": "^1.3.7",
    "webpack": "^2.3.1"
  }
}
